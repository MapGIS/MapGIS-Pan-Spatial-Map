{
  "name": "@mapgis/pan-spatial-map",
  "version": "0.0.4",
  "description": "一个更易于帮助全行业构建全空间一张图专业GIS应用的平台",
  "productName": "MapGIS Pan-Spatial Map",
  "author": "MapGIS",
  "license": "Apache-2.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "cd packages/pan-spatial-map-framework && yarn serve",
    "dev:mock": "concurrently \"yarn mock\" \"yarn serve\"",
    "build": "cd packages/pan-spatial-map-framework && yarn build",
    "serve": "cd packages/pan-spatial-map-framework && serve dist/spa",
    "serve:mock": "concurrently \"yarn mock\" \"yarn serve\"",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore ./",
    "lint:fix": "npm run lint -- --fix"
  },
  "devDependencies": {
    "concurrently": "^5.2.0"
  }
}
