{
  "name": "@mapgis/pan-spatial-map",
  "version": "0.1.0",
  "description": "一个更易于帮助全行业构建全空间一张图专业GIS应用的平台",
  "productName": "MapGIS Pan-Spatial Map",
  "author": "MapGIS",
  "license": "Apache-2.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "cd packages/pan-spatial-map-framework && yarn serve",
    "dev:mock": "concurrently \"yarn mock\" \"yarn dev\"",
    "build": "cd packages/pan-spatial-map-framework && yarn build",
    "build:portal": "cd packages/pan-spatial-map-framework && yarn build:portal",
    "serve": "cd packages/pan-spatial-map-framework && serve dist",
    "serve:mock": "concurrently \"yarn mock\" \"yarn serve\"",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore ./",
    "lint:fix": "npm run lint -- --fix",
    "mock": "cd packages/pan-spatial-map-mock-server/mock && ts-node-dev mock-server.ts",
    "svgo": "svgo -r -f icons --config=icons/svgo.yml",
    "limit": "cd packages/pan-spatial-map-framework && yarn fix-memory-limit"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "babel-eslint": "^10.0.1",
    "concurrently": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-vue": "^6.1.2",
    "svgo": "1.2.0"
  }
}
